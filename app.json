{
  "expo": {
    "name": "Fishnet",
    "slug": "Fishnet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/FishnetIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#369eff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/FishnetIcon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/images/FishnetIcon.png"
    },
    "plugins": [
      "expo-image-picker",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Fishnet to use your location."
        }
      ]
    ]
  }
}
